{
    "name": "jeroenvanrensen/laravel-share-links",
    "description": "Get social share links for 10+ platforms easily.",
    "keywords": [
        "jeroenvanrensen", "share", "social", "links", "laravel"
    ],
    "homepage": "https://github.com/jeroenvanrensen/laravel-share-links",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jeroen van Rensen",
            "email": "jmjrensen@jcjcj.nl",
            "homepage": "https://www.jeroenvanrensen.nl/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "JeroenvanRensen\\ShareLinks\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JeroenvanRensen\\ShareLinks\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JeroenvanRensen\\ShareLinks\\ShareLinksServiceProvider"
            ],
            "aliases": {
                "Share": "JeroenvanRensen\\ShareLinks\\ShareFacade"
            }
        }
    }
}
